{% extends 'domain/authentication/layout/base.html.twig' %}
{% block title 'Inscription' %}

{% block body %}
    <div class="nk-block-head">
        <div class="nk-block-head-content">
            <h5 class="nk-block-title fw-bolder">Inscription - Gestion de contacts</h5>
            <div class="nk-block-des"><p>Bon retour parmi nous, connectez-vous pour continuer !</p></div>
        </div>
    </div>

    {{ form_start(form) }}
